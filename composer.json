{
    "name": "xqueue/maileon-api-client",
    "description": "API-Client to XQ:Maileon",
    "version": "1.5.4",
    "keywords": ["XQueue", "Maileon"],
    "homepage": "https://www.maileon.de",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wanadis Devs",
            "email": "dev@maileon.hu"
        }
    ],
    "require": {
        "php": "^5.3.0 || ^7.0.0",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "Maileon\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    }
}